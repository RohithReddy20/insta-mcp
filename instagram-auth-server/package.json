{
  "name": "instagram-auth-server",
  "version": "1.0.0",
  "description": "HTTPS server for Instagram OAuth callback handling",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "generate-certs": "openssl req -nodes -new -x509 -keyout server.key -out server.cert -subj '/CN=localhost'"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "https": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "keywords": [
    "instagram",
    "oauth",
    "https",
    "server"
  ],
  "author": "",
  "license": "MIT"
}
