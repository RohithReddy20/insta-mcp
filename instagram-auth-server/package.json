{
  "name": "instagram-auth-server",
  "version": "1.0.0",
  "description": "HTTPS server for Instagram OAuth callback handling",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "generate-certs": "openssl req -nodes -new -x509 -keyout server.key -out server.cert -subj '/CN=localhost'"
  },
  "dependencies": {
    "express": "^4.19.2",
    "body-parser": "^1.20.2",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  },
  "keywords": [
    "instagram",
    "oauth",
    "https",
    "server"
  ],
  "author": "",
  "license": "MIT"
}
